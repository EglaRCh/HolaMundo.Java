pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/EglaRCh/HolaMundo.java.git'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
    post {
        always {
            archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true
        }
    }
}
